      <bpmn:script>System.out.println("Validating...");
m = execution.getVariable("msisdn");
execution.setVariable("validated", true);
offer = execution.getVariable("productOfferingId");
res = true;
if ( m == null ) {
  execution.setVariable("errorMessage", "No msisdn provided");
  res = false;
} else if ( offer == null) {
  execution.setVariable("errorMessage", "No productOfferingId provided");
  res = false;
}
execution.setVariable("validated", res);
if ( ! res ) {
  System.out.println("Validation failed!!!");
  throw new org.camunda.bpm.engine.delegate.BpmnError("ValidationError");
}</bpmn:script>
